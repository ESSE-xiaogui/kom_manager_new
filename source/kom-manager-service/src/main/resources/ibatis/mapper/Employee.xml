	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="Employee">
		<![CDATA[ INSERT INTO T_EMPLOYEE ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{empId}
			, #{empCode}
			, #{empName}
			, #{uId}
			, #{idNo}
			, #{companyId}
			, #{orgId}
			, #{dutyId}
			, #{upperCode}
			, #{upperName}
			, #{gender}
			, str_to_date(#{birthday},'%Y-%m-%d')
			, #{nation}
			, #{married}
			, #{phoneNo}
			, #{mobileNo}
			, #{email}
			, #{nativePlace}
			, #{zipCode}
			, #{inService}
			, #{remark}
			, #{createdBy}
			, str_to_date(#{createdTime},'%Y-%m-%d %H:%i:%s')
			, #{updatedBy}
			, str_to_date(#{updatedTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="Employee">
		<![CDATA[ UPDATE T_EMPLOYEE SET 
			 ID=#{id} 
			, EMP_ID=#{empId} 
			, EMP_CODE=#{empCode} 
			, EMP_NAME=#{empName} 
			, U_ID=#{uId}
			, ID_NO=#{idNo} 
			, COMPANY_ID=#{companyId} 
			, ORG_ID=#{orgId} 
			, DUTY_ID=#{dutyId} 
			, UPPER_CODE=#{upperCode} 
			, UPPER_NAME=#{upperName} 
			, GENDER=#{gender} 
			, BIRTHDAY=str_to_date(#{birthday},'%Y-%m-%d')
			, NATION=#{nation} 
			, MARRIED=#{married} 
			, PHONE_NO=#{phoneNo} 
			, MOBILE_NO=#{mobileNo} 
			, EMAIL=#{email} 
			, NATIVE_PLACE=#{nativePlace} 
			, ZIP_CODE=#{zipCode} 
			, IN_SERVICE=#{inService} 
			, REMARK=#{remark} 
			, CREATED_BY=#{createdBy} 
			, CREATED_TIME=str_to_date(#{createdTime},'%Y-%m-%d %H:%i:%s')
			, UPDATED_BY=#{updatedBy} 
			, UPDATED_TIME=str_to_date(#{updatedTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_EMPLOYEE WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_EMPLOYEE WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="Employee">
		DELETE FROM T_EMPLOYEE WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="EmployeeResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_EMPLOYEE WHERE ID = #{id}  
	</select>
