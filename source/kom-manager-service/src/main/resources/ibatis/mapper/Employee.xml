	<select id="listByProperty" parameterType="Employee" resultMap="EmployeeResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_EMPLOYEE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listByProp" parameterType="Employee" resultMap="EmpResponseDtoResultMapper">
		 SELECT e.ID,EMP_CODE, EMP_NAME, ID_NO,ORG_NAME,d.DUTY_NAME, UPPER_CODE, UPPER_NAME,
		 GENDER, BIRTHDAY, NATION, MARRIED, PHONE_NO,EMAIL, IN_SERVICE FROM T_EMPLOYEE e 
		 LEFT JOIN T_DUTY d ON d.ID = e.DUTY_ID
		 LEFT JOIN T_ORGANIZATION o ON e.ORG_ID = o.ID
		 WHERE e.ID = #{id}
	</select>
	<select id="listPaginationByProperty" parameterType="Employee" resultMap="EmployeeResultMapper">
		 SELECT e.ID,EMP_CODE, EMP_NAME, ID_NO,ORG_NAME,d.DUTY_NAME, UPPER_CODE, UPPER_NAME,
		 GENDER, BIRTHDAY, NATION, MARRIED, PHONE_NO,EMAIL, IN_SERVICE,e.CREATED_BY,
		 e.CREATED_TIME FROM T_EMPLOYEE e 
		 LEFT JOIN T_DUTY d ON d.ID = e.DUTY_ID
		 LEFT JOIN T_ORGANIZATION o ON e.ORG_ID = o.ID
		 WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="Employee" resultType="int">
		SELECT count(1) AS totalcount FROM T_EMPLOYEE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="getEmpInfo" parameterType="String" resultMap="EmpResponseDtoResultMapper">
		SELECT ID,EMP_NAME,EMP_CODE FROM T_EMPLOYEE WHERE EMP_NAME LIKE CONCAT('%',#{empName},'%')
	</select>
