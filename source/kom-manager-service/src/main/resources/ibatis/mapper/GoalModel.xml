<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.GoalModelMapper">
	<resultMap id="GoalModelResultMapper" type="GoalModel">
		<result column="ID" property="id"/>
		<result column="COMPANY_ID" property="companyId"/>
		<result column="GOAL_MONTH" property="goalMonth"/>
		<result column="SHOP_ID" property="shopId"/>
		<result column="SHOP_CODE" property="shopCode"/>
		<result column="MODEL_ID" property="modelId"/>
		<result column="MODEL_CODE" property="modelCode"/>
		<result column="SALE_TARGET" property="saleTarget"/>
		<result column="SALE_CURRENT" property="saleCurrent"/>
		<result column="ACHIEVED" property="achieved"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<resultMap id="StatShopModelSaleDtoResultMapper" type="StatShopModelSaleDto">
		<result column="MODEL_CODE" property="modelCode"/>
		<result column="SALE_TARGET" property="targetSaleQty"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, COMPANY_ID
		, GOAL_MONTH
		, SHOP_ID
		, SHOP_CODE
		, MODEL_ID
		, MODEL_CODE
		, SALE_TARGET
		, SALE_CURRENT
		, ACHIEVED
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 COMPANY_ID
		, GOAL_MONTH
		, SHOP_ID
		, SHOP_CODE
		, MODEL_ID
		, MODEL_CODE
		, SALE_TARGET
		, SALE_CURRENT
		, ACHIEVED
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, COMPANY_ID
		, date_format(GOAL_MONTH, '%Y-%m-%d %H:%i:%s') GOAL_MONTH
		, SHOP_ID
		, SHOP_CODE
		, MODEL_ID
		, MODEL_CODE
		, SALE_TARGET
		, SALE_CURRENT
		, ACHIEVED
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="companyId!= null and companyId!= ''"> 
			AND COMPANY_ID=#{companyId}
		</if>
		<if test="goalMonth!= null and goalMonth!= ''"> 
			AND GOAL_MONTH=str_to_date(#{goalMonth},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="shopId!= null and shopId!= ''"> 
			AND SHOP_ID=#{shopId}
		</if>
		<if test="shopCode!= null and shopCode!= ''"> 
			AND SHOP_CODE=#{shopCode}
		</if>
		<if test="modelId!= null and modelId!= ''"> 
			AND MODEL_ID=#{modelId}
		</if>
		<if test="modelCode!= null and modelCode!= ''"> 
			AND MODEL_CODE=#{modelCode}
		</if>
		<if test="saleTarget!= null and saleTarget!= ''"> 
			AND SALE_TARGET=#{saleTarget}
		</if>
		<if test="saleCurrent!= null and saleCurrent!= ''"> 
			AND SALE_CURRENT=#{saleCurrent}
		</if>
		<if test="achieved!= null and achieved!= ''"> 
			AND ACHIEVED=#{achieved}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="goalModel != null">
            <if test="goalModel.id!= null and goalModel.id!= ''">
				AND ID=#{goalModel.id}
            </if>
            <if test="goalModel.companyId!= null and goalModel.companyId!= ''">
				AND COMPANY_ID=#{goalModel.companyId}
            </if>
            <if test="goalModel.goalMonth!= null and goalModel.goalMonth!= ''">
				AND GOAL_MONTH=str_to_date(#{goalModel.goalMonth},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="goalModel.shopId!= null and goalModel.shopId!= ''">
				AND SHOP_ID=#{goalModel.shopId}
            </if>
            <if test="goalModel.shopCode!= null and goalModel.shopCode!= ''">
				AND SHOP_CODE=#{goalModel.shopCode}
            </if>
            <if test="goalModel.modelId!= null and goalModel.modelId!= ''">
				AND MODEL_ID=#{goalModel.modelId}
            </if>
            <if test="goalModel.modelCode!= null and goalModel.modelCode!= ''">
				AND MODEL_CODE=#{goalModel.modelCode}
            </if>
            <if test="goalModel.saleTarget!= null and goalModel.saleTarget!= ''">
				AND SALE_TARGET=#{goalModel.saleTarget}
            </if>
            <if test="goalModel.saleCurrent!= null and goalModel.saleCurrent!= ''">
				AND SALE_CURRENT=#{goalModel.saleCurrent}
            </if>
            <if test="goalModel.achieved!= null and goalModel.achieved!= ''">
				AND ACHIEVED=#{goalModel.achieved}
            </if>
            <if test="goalModel.remark!= null and goalModel.remark!= ''">
				AND REMARK=#{goalModel.remark}
            </if>
            <if test="goalModel.createBy!= null and goalModel.createBy!= ''">
				AND CREATE_BY=#{goalModel.createBy}
            </if>
            <if test="goalModel.createTime!= null and goalModel.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{goalModel.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="goalModel.updateBy!= null and goalModel.updateBy!= ''">
				AND UPDATE_BY=#{goalModel.updateBy}
            </if>
            <if test="goalModel.updateTime!= null and goalModel.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{goalModel.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="goalModel.version!= null and goalModel.version!= ''">
				AND VERSION=#{goalModel.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'companyId'">
				COMPANY_ID ${value}
            </when>
        	<when test="key == 'goalMonth'">
				GOAL_MONTH ${value}
            </when>
        	<when test="key == 'shopId'">
				SHOP_ID ${value}
            </when>
        	<when test="key == 'shopCode'">
				SHOP_CODE ${value}
            </when>
        	<when test="key == 'modelId'">
				MODEL_ID ${value}
            </when>
        	<when test="key == 'modelCode'">
				MODEL_CODE ${value}
            </when>
        	<when test="key == 'saleTarget'">
				SALE_TARGET ${value}
            </when>
        	<when test="key == 'saleCurrent'">
				SALE_CURRENT ${value}
            </when>
        	<when test="key == 'achieved'">
				ACHIEVED ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="GoalModel">
		<![CDATA[ INSERT INTO T_GOAL_MODEL ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{companyId}
			, str_to_date(#{goalMonth},'%Y-%m-%d %H:%i:%s')
			, #{shopId}
			, #{shopCode}
			, #{modelId}
			, #{modelCode}
			, #{saleTarget}
			, #{saleCurrent}
			, #{achieved}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="GoalModel">
		<![CDATA[ UPDATE T_GOAL_MODEL SET 
			 ID=#{id} 
			, COMPANY_ID=#{companyId} 
			, GOAL_MONTH=str_to_date(#{goalMonth},'%Y-%m-%d %H:%i:%s')
			, SHOP_ID=#{shopId} 
			, SHOP_CODE=#{shopCode} 
			, MODEL_ID=#{modelId} 
			, MODEL_CODE=#{modelCode} 
			, SALE_TARGET=#{saleTarget} 
			, SALE_CURRENT=#{saleCurrent} 
			, ACHIEVED=#{achieved} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_GOAL_MODEL WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_GOAL_MODEL WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="GoalModel">
		DELETE FROM T_GOAL_MODEL WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="GoalModelResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_GOAL_MODEL WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="GoalModelResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_GOAL_MODEL
	</select>
	<select id="listByProperty" parameterType="GoalModel" resultMap="GoalModelResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_GOAL_MODEL WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="GoalModel" resultMap="GoalModelResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_GOAL_MODEL WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="GoalModel" resultType="int">
		SELECT count(1) AS totalcount FROM T_GOAL_MODEL WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	
	<select id="queryModelSaleTargetByShopId" resultMap="StatShopModelSaleDtoResultMapper">
		SELECT MODEL_CODE, SALE_TARGET FROM T_GOAL_MODEL WHERE SHOP_ID = #{shopId} AND GOAL_MONTH = #{goalMonth} 
		AND MODEL_CODE IN 
		 <foreach item="modelCode" collection="modelCodeList" separator="," open="("
			close=")" index="">
			#{modelCode}
		</foreach>
	</select>
</mapper>