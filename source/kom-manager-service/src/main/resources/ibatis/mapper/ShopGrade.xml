<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.ShopGradeMapper">
	<resultMap id="ShopGradeResultMapper" type="ShopGrade">
		<result column="ID" property="id"/>
		<result column="GRADE_NAME" property="gradeName"/>
		<result column="BRAND_CODE" property="brandCode"/>
		<result column="SALE_QTY" property="saleQTY"/>
		<result column="COMPANY_ID" property="companyId"/>
		<result column="IS_INACTIVE" property="isInactive"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_DATE" property="createDate"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_DATE" property="updateDate"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, GRADE_NAME
		, BRAND_CODE
		, SALE_QTY
		, COMPANY_ID
		, IS_INACTIVE
		, REMARK
		, CREATE_BY
		, CREATE_DATE
		, UPDATE_BY
		, UPDATE_DATE
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 GRADE_NAME
		, BRAND_CODE
		, SALE_QTY
		, COMPANY_ID
		, IS_INACTIVE
		, REMARK
		, CREATE_BY
		, CREATE_DATE
		, UPDATE_BY
		, UPDATE_DATE
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, GRADE_NAME
		, BRAND_CODE
		, SALE_QTY
		, COMPANY_ID
		, IS_INACTIVE
		, REMARK
		, CREATE_BY
		, date_format(CREATE_DATE, '%Y-%m-%d %H:%i:%s') CREATE_DATE
		, UPDATE_BY
		, date_format(UPDATE_DATE, '%Y-%m-%d %H:%i:%s') UPDATE_DATE
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="gradeName!= null and gradeName!= ''"> 
			AND GRADE_NAME=#{gradeName}
		</if>
		<if test="brandCode!= null and brandCode!= ''"> 
			AND BRAND_CODE=#{brandCode}
		</if>
		<if test="saleQTY!= null and saleQTY!= ''"> 
			AND SALE_QTY=#{saleQTY}
		</if>
		<if test="companyId!= null and companyId!= ''"> 
			AND COMPANY_ID=#{companyId}
		</if>
		<if test="isInactive!= null and isInactive!= ''"> 
			AND IS_INACTIVE=#{isInactive}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createDate!= null and createDate!= ''"> 
			AND CREATE_DATE=str_to_date(#{createDate},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateDate!= null and updateDate!= ''"> 
			AND UPDATE_DATE=str_to_date(#{updateDate},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="shopGrade != null">
            <if test="shopGrade.id!= null and shopGrade.id!= ''">
				AND ID=#{shopGrade.id}
            </if>
            <if test="shopGrade.gradeName!= null and shopGrade.gradeName!= ''">
				AND GRADE_NAME=#{shopGrade.gradeName}
            </if>
            <if test="shopGrade.brandCode!= null and shopGrade.brandCode!= ''">
				AND BRAND_CODE=#{shopGrade.brandCode}
            </if>
            <if test="shopGrade.saleQTY!= null and shopGrade.saleQTY!= ''">
				AND SALE_QTY=#{shopGrade.saleQTY}
            </if>
            <if test="shopGrade.companyId!= null and shopGrade.companyId!= ''">
				AND COMPANY_ID=#{shopGrade.companyId}
            </if>
            <if test="shopGrade.isInactive!= null and shopGrade.isInactive!= ''">
				AND IS_INACTIVE=#{shopGrade.isInactive}
            </if>
            <if test="shopGrade.remark!= null and shopGrade.remark!= ''">
				AND REMARK=#{shopGrade.remark}
            </if>
            <if test="shopGrade.createBy!= null and shopGrade.createBy!= ''">
				AND CREATE_BY=#{shopGrade.createBy}
            </if>
            <if test="shopGrade.createDate!= null and shopGrade.createDate!= ''">
				AND CREATE_DATE=str_to_date(#{shopGrade.createDate},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="shopGrade.updateBy!= null and shopGrade.updateBy!= ''">
				AND UPDATE_BY=#{shopGrade.updateBy}
            </if>
            <if test="shopGrade.updateDate!= null and shopGrade.updateDate!= ''">
				AND UPDATE_DATE=str_to_date(#{shopGrade.updateDate},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="shopGrade.version!= null and shopGrade.version!= ''">
				AND VERSION=#{shopGrade.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'gradeName'">
				GRADE_NAME ${value}
            </when>
            <when test="key == 'brandCode'">
				BRAND_CODE ${value}
            </when>
            <when test="key == 'saleQTY'">
				SALE_QTY ${value}
            </when>
        	<when test="key == 'companyId'">
				COMPANY_ID ${value}
            </when>
            <when test="key == 'isInactive'">
				IS_INACTIVE ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createDate'">
				CREATE_DATE ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateDate'">
				UPDATE_DATE ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="ShopGrade">
		<![CDATA[ INSERT INTO T_SHOP_GRADE ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{gradeName}
			, #{brandCode}
			, #{saleQTY}
			, #{companyId}
			, #{isInactive}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createDate},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateDate},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="ShopGrade">
		<![CDATA[ UPDATE T_SHOP_GRADE SET 
			 ID=#{id} 
			, GRADE_NAME=#{gradeName}
			, SALE_QTY=#{saleQTY} 
			, COMPANY_ID=#{companyId} 
			, REMARK=#{remark} 
			, IS_INACTIVE=#{isInactive}
			, CREATE_BY=#{createBy} 
			, CREATE_DATE=str_to_date(#{createDate},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_DATE=str_to_date(#{updateDate},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_SHOP_GRADE WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_SHOP_GRADE WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="ShopGrade">
		DELETE FROM T_SHOP_GRADE WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="ShopGradeResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_SHOP_GRADE WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="ShopGradeResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_SHOP_GRADE
	</select>
	<select id="listByProperty" parameterType="ShopGrade" resultMap="ShopGradeResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_SHOP_GRADE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="ShopGrade" resultMap="ShopGradeResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_SHOP_GRADE WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="ShopGrade" resultType="int">
		SELECT count(1) AS totalcount FROM T_SHOP_GRADE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="findShopGrade" parameterType="java.lang.Long"
		resultMap="ShopGradeResultMapper">
		SELECT b.ID,b.GRADE_NAME FROM T_SHOP_GRADE b
		LEFT JOIN T_SHOP p ON p.GRADE_ID = b.ID
		LEFT JOIN T_SALE s ON s.SHOP_ID = p.ID
		LEFT JOIN T_SALE_ITEM i ON i.SALE_ID = s.ID
		WHERE i.ID = #{saleId}
	</select>
</mapper>