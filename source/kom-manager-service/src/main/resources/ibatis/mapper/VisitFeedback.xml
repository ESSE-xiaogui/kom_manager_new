<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.VisitFeedbackMapper">
	<resultMap id="VisitFeedbackResultMapper" type="VisitFeedback">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="ISSUE" property="issue"/>
		<result column="ACTION" property="action"/>
		<result column="IMG_FIRST_URL" property="imgFirstUrl"/>
		<result column="IMG_SECOND_URL" property="imgSecondUrl"/>
		<result column="IMG_THIRD_URL" property="imgThirdUrl"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, VISIT_ID
		, ISSUE
		, ACTION
		, IMG_FIRST_URL
		, IMG_SECOND_URL
		, IMG_THIRD_URL
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 VISIT_ID
		, ISSUE
		, ACTION
		, IMG_FIRST_URL
		, IMG_SECOND_URL
		, IMG_THIRD_URL
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, VISIT_ID
		, ISSUE
		, ACTION
		, IMG_FIRST_URL
		, IMG_SECOND_URL
		, IMG_THIRD_URL
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="visitId!= null and visitId!= ''"> 
			AND VISIT_ID=#{visitId}
		</if>
		<if test="issue!= null and issue!= ''"> 
			AND ISSUE=#{issue}
		</if>
		<if test="action!= null and action!= ''"> 
			AND ACTION=#{action}
		</if>
		<if test="imgFirstUrl!= null and imgFirstUrl!= ''"> 
			AND IMG_FIRST_URL=#{imgFirstUrl}
		</if>
		<if test="imgSecondUrl!= null and imgSecondUrl!= ''"> 
			AND IMG_SECOND_URL=#{imgSecondUrl}
		</if>
		<if test="imgThirdUrl!= null and imgThirdUrl!= ''"> 
			AND IMG_THIRD_URL=#{imgThirdUrl}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="visitFeedback != null">
            <if test="visitFeedback.id!= null and visitFeedback.id!= ''">
				AND ID=#{visitFeedback.id}
            </if>
            <if test="visitFeedback.visitId!= null and visitFeedback.visitId!= ''">
				AND VISIT_ID=#{visitFeedback.visitId}
            </if>
            <if test="visitFeedback.issue!= null and visitFeedback.issue!= ''">
				AND ISSUE=#{visitFeedback.issue}
            </if>
            <if test="visitFeedback.action!= null and visitFeedback.action!= ''">
				AND ACTION=#{visitFeedback.action}
            </if>
            <if test="visitFeedback.imgFirstUrl!= null and visitFeedback.imgFirstUrl!= ''">
				AND IMG_FIRST_URL=#{visitFeedback.imgFirstUrl}
            </if>
            <if test="visitFeedback.imgSecondUrl!= null and visitFeedback.imgSecondUrl!= ''">
				AND IMG_SECOND_URL=#{visitFeedback.imgSecondUrl}
            </if>
            <if test="visitFeedback.imgThirdUrl!= null and visitFeedback.imgThirdUrl!= ''">
				AND IMG_THIRD_URL=#{visitFeedback.imgThirdUrl}
            </if>
            <if test="visitFeedback.remark!= null and visitFeedback.remark!= ''">
				AND REMARK=#{visitFeedback.remark}
            </if>
            <if test="visitFeedback.createBy!= null and visitFeedback.createBy!= ''">
				AND CREATE_BY=#{visitFeedback.createBy}
            </if>
            <if test="visitFeedback.createTime!= null and visitFeedback.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{visitFeedback.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitFeedback.updateBy!= null and visitFeedback.updateBy!= ''">
				AND UPDATE_BY=#{visitFeedback.updateBy}
            </if>
            <if test="visitFeedback.updateTime!= null and visitFeedback.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{visitFeedback.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitFeedback.version!= null and visitFeedback.version!= ''">
				AND VERSION=#{visitFeedback.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'visitId'">
				VISIT_ID ${value}
            </when>
        	<when test="key == 'issue'">
				ISSUE ${value}
            </when>
        	<when test="key == 'action'">
				ACTION ${value}
            </when>
        	<when test="key == 'imgFirstUrl'">
				IMG_FIRST_URL ${value}
            </when>
        	<when test="key == 'imgSecondUrl'">
				IMG_SECOND_URL ${value}
            </when>
        	<when test="key == 'imgThirdUrl'">
				IMG_THIRD_URL ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="VisitFeedback">
		<![CDATA[ INSERT INTO T_VISIT_FEEDBACK ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{visitId}
			, #{issue}
			, #{action}
			, #{imgFirstUrl}
			, #{imgSecondUrl}
			, #{imgThirdUrl}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="VisitFeedback">
		<![CDATA[ UPDATE T_VISIT_FEEDBACK SET 
			 ID=#{id} 
			, VISIT_ID=#{visitId} 
			, ISSUE=#{issue} 
			, ACTION=#{action} 
			, IMG_FIRST_URL=#{imgFirstUrl} 
			, IMG_SECOND_URL=#{imgSecondUrl} 
			, IMG_THIRD_URL=#{imgThirdUrl} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_VISIT_FEEDBACK WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_VISIT_FEEDBACK WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="VisitFeedback">
		DELETE FROM T_VISIT_FEEDBACK WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="VisitFeedbackResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_VISIT_FEEDBACK WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="VisitFeedbackResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_VISIT_FEEDBACK
	</select>
	<select id="listByProperty" parameterType="VisitFeedback" resultMap="VisitFeedbackResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_FEEDBACK WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="VisitFeedback" resultMap="VisitFeedbackResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_FEEDBACK WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="VisitFeedback" resultType="int">
		SELECT count(1) AS totalcount FROM T_VISIT_FEEDBACK WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
</mapper>