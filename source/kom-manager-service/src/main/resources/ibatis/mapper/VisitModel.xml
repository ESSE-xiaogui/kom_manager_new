<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.VisitModelMapper">
	<resultMap id="VisitModelResultMapper" type="VisitModel">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="BRAND_ID" property="brandId"/>
		<result column="MODEL_ID" property="modelId"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, VISIT_ID
		, BRAND_ID
		, MODEL_ID
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 VISIT_ID
		, BRAND_ID
		, MODEL_ID
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, VISIT_ID
		, BRAND_ID
		, MODEL_ID
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="visitId!= null and visitId!= ''"> 
			AND VISIT_ID=#{visitId}
		</if>
		<if test="brandId!= null and brandId!= ''"> 
			AND BRAND_ID=#{brandId}
		</if>
		<if test="modelId!= null and modelId!= ''"> 
			AND MODEL_ID=#{modelId}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="visitModel != null">
            <if test="visitModel.id!= null and visitModel.id!= ''">
				AND ID=#{visitModel.id}
            </if>
            <if test="visitModel.visitId!= null and visitModel.visitId!= ''">
				AND VISIT_ID=#{visitModel.visitId}
            </if>
            <if test="visitModel.brandId!= null and visitModel.brandId!= ''">
				AND BRAND_ID=#{visitModel.brandId}
            </if>
            <if test="visitModel.modelId!= null and visitModel.modelId!= ''">
				AND MODEL_ID=#{visitModel.modelId}
            </if>
            <if test="visitModel.remark!= null and visitModel.remark!= ''">
				AND REMARK=#{visitModel.remark}
            </if>
            <if test="visitModel.createBy!= null and visitModel.createBy!= ''">
				AND CREATE_BY=#{visitModel.createBy}
            </if>
            <if test="visitModel.createTime!= null and visitModel.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{visitModel.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitModel.updateBy!= null and visitModel.updateBy!= ''">
				AND UPDATE_BY=#{visitModel.updateBy}
            </if>
            <if test="visitModel.updateTime!= null and visitModel.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{visitModel.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitModel.version!= null and visitModel.version!= ''">
				AND VERSION=#{visitModel.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'visitId'">
				VISIT_ID ${value}
            </when>
        	<when test="key == 'brandId'">
				BRAND_ID ${value}
            </when>
        	<when test="key == 'modelId'">
				MODEL_ID ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="VisitModel">
		<![CDATA[ INSERT INTO T_VISIT_MODEL ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{visitId}
			, #{shopId}
			, #{brandId}
			, #{modelId}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="VisitModel">
		<![CDATA[ UPDATE T_VISIT_MODEL SET 
			 ID=#{id} 
			, VISIT_ID=#{visitId} 
			, SHOP_ID=#{shopId} 
			, BRAND_ID=#{brandId} 
			, MODEL_ID=#{modelId} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_VISIT_MODEL WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_VISIT_MODEL WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="VisitModel">
		DELETE FROM T_VISIT_MODEL WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="VisitModelResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_VISIT_MODEL WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="VisitModelResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_VISIT_MODEL
	</select>
	<select id="listByProperty" parameterType="VisitModel" resultMap="VisitModelResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_MODEL WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="VisitModel" resultMap="VisitModelResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_MODEL WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="VisitModel" resultType="int">
		SELECT count(1) AS totalcount FROM T_VISIT_MODEL WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
</mapper>