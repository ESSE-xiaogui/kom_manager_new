<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.VisitScoreMapper">
	<resultMap id="VisitScoreResultMapper" type="VisitScore">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="SHOP_SCORE" property="shopScore"/>
		<result column="SALE_SCORE" property="saleScore"/>
		<result column="STAFF_SCORE" property="staffScore"/>
		<result column="TOTAL_SCORE" property="totalScore"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<resultMap id="VisitScoreDetailInfoDtoResultMapper" type="VisitScoreDetailInfoDto">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="COMPANY_CODE" property="companyCode"/>
		<result column="COUNTRY_NAME" property="countryName"/>
		<result column="CITY_NAME" property="cityName"/>
		<result column="SHOP_CODE" property="shopCode"/>
		<result column="SHOP_NAME" property="shopName"/>
		<result column="EMP_NAME" property="empName"/>
		<result column="STAFF_SCORE" property="staffScore"/>
		<result column="SHOP_SCORE" property="shopScore"/>
		<result column="SALE_SCORE" property="saleScore"/>
		<result column="STAFF_SCORE" property="staffScore"/>
		<result column="TOTAL_SCORE" property="totalScore"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	
	<sql id="commonColumns">
		 ID
		, VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumn">
		 vs.ID
		, vs.VISIT_ID
		, c.COMPANY_CODE
		, r1.REGION_NAME AS COUNTRY_NAME
		, r2.REGION_NAME AD CITY_NAME
		, s.SHOP_CODE
		, s.SHOP_NAME
		, e.EPM_NAME
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="visitId!= null and visitId!= ''"> 
			AND VISIT_ID=#{visitId}
		</if>
		<if test="shopScore!= null and shopScore!= ''"> 
			AND SHOP_SCORE=#{shopScore}
		</if>
		<if test="saleScore!= null and saleScore!= ''"> 
			AND SALE_SCORE=#{saleScore}
		</if>
		<if test="staffScore!= null and staffScore!= ''"> 
			AND STAFF_SCORE=#{staffScore}
		</if>
		<if test="totalScore!= null and totalScore!= ''"> 
			AND TOTAL_SCORE=#{totalScore}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="visitScoreDetailInfo != null">
            <if test="visitScoreDetailInfo.id!= null and visitScoreDetailInfo.id!= ''">
				AND ID=#{visitScoreDetailInfo.id}
            </if>
            <if test="visitScoreDetailInfo.visitId!= null and visitScoreDetailInfo.visitId!= ''">
				AND VISIT_ID=#{visitScoreDetailInfo.visitId}
            </if>
            <if test="visitScoreDetailInfo.companyId!= null and visitScoreDetailInfo.companyId!= ''">
				AND COMPANY_ID=#{visitScoreDetailInfo.companyId}
            </if>
            <if test="visitScoreDetailInfo.regionId!= null and visitScoreDetailInfo.regionId!= ''">
				AND r1.ID=#{visitScoreDetailInfo.regionId}
            </if>
             <if test="visitScoreDetailInfo.shopCode!= null and visitScoreDetailInfo.shopCode!= ''">
				AND SHOP_CODE=#{visitScoreDetailInfo.shopCode}
            </if>
            <if test="visitScoreDetailInfo.shopName!= null and visitScoreDetailInfo.shopName!= ''">
				AND SHOP_NAME=#{visitScoreDetailInfo.shopName}
            </if>
            <if test="visitScoreDetailInfo.shopScore!= null and visitScoreDetailInfo.shopScore!= ''">
				AND SHOP_SCORE=#{visitScoreDetailInfo.shopScore}
            </if>
            <if test="visitScoreDetailInfo.saleScore!= null and visitScoreDetailInfo.saleScore!= ''">
				AND SALE_SCORE=#{visitScoreDetailInfo.saleScore}
            </if>
            <if test="visitScoreDetailInfo.staffScore!= null and visitScoreDetailInfo.staffScore!= ''">
				AND STAFF_SCORE=#{visitScoreDetailInfo.staffScore}
            </if>
            <if test="visitScoreDetailInfo.totalScore!= null and visitScoreDetailInfo.totalScore!= ''">
				AND TOTAL_SCORE=#{visitScoreDetailInfo.totalScore}
            </if>
            <if test="visitScoreDetailInfo.totalScoreStart!= null and visitScoreDetailInfo.totalScoreStart!= ''">
				AND TOTAL_SCORE &gt;=#{visitScoreDetailInfo.totalScoreStart}
            </if>
            <if test="visitScoreDetailInfo.totalScoreEnd!= null and visitScoreDetailInfo.totalScoreEnd!= ''">
				AND TOTAL_SCORE &lt;=#{visitScoreDetailInfo.totalScoreEnd}
            </if>
            <if test="visitScoreDetailInfo.empName!= null and visitScoreDetailInfo.empName!= ''">
				AND EMP_NAME=#{visitScoreDetailInfo.empName}
            </if>
            <if test="visitScoreDetailInfo.remark!= null and visitScoreDetailInfo.remark!= ''">
				AND REMARK=#{visitScoreDetailInfo.remark}
            </if>
            <if test="visitScoreDetailInfo.createBy!= null and visitScoreDetailInfo.createBy!= ''">
				AND CREATE_BY=#{visitScoreDetailInfo.createBy}
            </if>
            <if test="visitScoreDetailInfo.createTime!= null and visitScoreDetailInfo.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{visitScoreDetailInfo.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreDetailInfo.createTimeStart!= null and visitScoreDetailInfo.createTimeStart!= ''">
				AND CREATE_TIME &gt;=str_to_date(#{visitScoreDetailInfo.createTimeStart},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreDetailInfo.createTimeEnd!= null and visitScoreDetailInfo.createTimeEnd!= ''">
				AND CREATE_TIME &lt;=str_to_date(#{visitScoreDetailInfo.createTimeEnd},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreDetailInfo.updateBy!= null and visitScoreDetailInfo.updateBy!= ''">
				AND UPDATE_BY=#{visitScoreDetailInfo.updateBy}
            </if>
            <if test="visitScoreDetailInfo.updateTime!= null and visitScoreDetailInfo.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{visitScoreDetailInfo.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreDetailInfo.version!= null and visitScoreDetailInfo.version!= ''">
				AND VERSION=#{visitScoreDetailInfo.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'visitId'">
				VISIT_ID ${value}
            </when>
        	<when test="key == 'shopScore'">
				SHOP_SCORE ${value}
            </when>
        	<when test="key == 'saleScore'">
				SALE_SCORE ${value}
            </when>
        	<when test="key == 'staffScore'">
				STAFF_SCORE ${value}
            </when>
        	<when test="key == 'totalScore'">
				TOTAL_SCORE ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="VisitScore">
		<![CDATA[ INSERT INTO T_VISIT_SCORE ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{visitId}
			, #{shopScore}
			, #{saleScore}
			, #{staffScore}
			, #{totalScore}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="VisitScore">
		<![CDATA[ UPDATE T_VISIT_SCORE SET 
			 ID=#{id} 
			, VISIT_ID=#{visitId} 
			, SHOP_SCORE=#{shopScore} 
			, SALE_SCORE=#{saleScore} 
			, STAFF_SCORE=#{staffScore} 
			, TOTAL_SCORE=#{totalScore} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_VISIT_SCORE WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_VISIT_SCORE WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="VisitScore">
		DELETE FROM T_VISIT_SCORE WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_VISIT_SCORE
	</select>
	<select id="listByProperty" parameterType="VisitScore" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="VisitScoreDetailInfoDto" resultMap="VisitScoreDetailInfoDtoResultMapper">
		SELECT 
		<include refid="commonProcessDateColumn"/>
		FROM T_VISIT_SCORE vs
		LEFT JOIN T_VISIT v ON v.ID = vs.VISIT_ID
		LEFT JOIN T_COMPANY c ON c.ID = v.COMPANY_ID
		LEFT JOIN T_SHOP s ON s.ID = vs.SHOP_ID
		LEFT JOIN T_REGION r1 ON r1.ID = s.COUNTRY
		LEFT JOIN T_REGION r2 ON r2.ID = s.CITY
		LEFT JOIN T_USER u ON u.USER_CODE = vs.CREATE_BY
		LEFT JOIN T_EMPLOYEE e ON e.U_ID = u.ID
		WHERE 1=1
		<if test="companyId!= null and companyId!= ''"> 
			AND v.COMPANY_ID =#{companyId}
		</if>
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="VisitScore" resultType="int">
		SELECT count(1) AS totalcount FROM T_VISIT_SCORE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listVisitScoreByProperty" parameterType="VisitScoreDetailInfoDto" resultMap="VisitScoreDetailInfoDtoResultMapper">
		SELECT 
		<include refid="commonProcessDateColumn"/>
		FROM T_VISIT_SCORE vs
		LEFT JOIN T_VISIT v ON v.ID = vs.VISIT_ID
		LEFT JOIN T_COMPANY c ON c.ID = v.COMPANY_ID
		LEFT JOIN T_SHOP s ON s.ID = vs.SHOP_ID
		LEFT JOIN T_REGION r1 ON r1.ID = s.COUNTRY
		LEFT JOIN T_REGION r2 ON r2.ID = s.CITY
		LEFT JOIN T_USER u ON u.USER_CODE = vs.CREATE_BY
		LEFT JOIN T_EMPLOYEE e ON e.U_ID = u.ID
		WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	ORDER BY vs.CREATE_TIME
	</select>
</mapper>