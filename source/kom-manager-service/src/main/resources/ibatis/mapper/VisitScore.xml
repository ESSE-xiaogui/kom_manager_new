<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.VisitScoreMapper">
	<resultMap id="VisitScoreResultMapper" type="VisitScore">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="SHOP_SCORE" property="shopScore"/>
		<result column="SALE_SCORE" property="saleScore"/>
		<result column="STAFF_SCORE" property="staffScore"/>
		<result column="TOTAL_SCORE" property="totalScore"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<resultMap id="VisitScoreDtoResultMapper" type="VisitScoreDto">
		<result column="ID" property="id"/>
		<result column="VISIT_ID" property="visitId"/>
		<result column="SHOP_SCORE" property="shopScore"/>
		<result column="SALE_SCORE" property="saleScore"/>
		<result column="STAFF_SCORE" property="staffScore"/>
		<result column="TOTAL_SCORE" property="totalScore"/>
		<result column="REMARK" property="remark"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, VISIT_ID
		, SHOP_SCORE
		, SALE_SCORE
		, STAFF_SCORE
		, TOTAL_SCORE
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="visitId!= null and visitId!= ''"> 
			AND VISIT_ID=#{visitId}
		</if>
		<if test="shopScore!= null and shopScore!= ''"> 
			AND SHOP_SCORE=#{shopScore}
		</if>
		<if test="saleScore!= null and saleScore!= ''"> 
			AND SALE_SCORE=#{saleScore}
		</if>
		<if test="staffScore!= null and staffScore!= ''"> 
			AND STAFF_SCORE=#{staffScore}
		</if>
		<if test="totalScore!= null and totalScore!= ''"> 
			AND TOTAL_SCORE=#{totalScore}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="visitScore != null">
            <if test="visitScore.id!= null and visitScore.id!= ''">
				AND ID=#{visitScore.id}
            </if>
            <if test="visitScore.visitId!= null and visitScore.visitId!= ''">
				AND VISIT_ID=#{visitScore.visitId}
            </if>
            <if test="visitScore.shopScore!= null and visitScore.shopScore!= ''">
				AND SHOP_SCORE=#{visitScore.shopScore}
            </if>
            <if test="visitScore.saleScore!= null and visitScore.saleScore!= ''">
				AND SALE_SCORE=#{visitScore.saleScore}
            </if>
            <if test="visitScore.staffScore!= null and visitScore.staffScore!= ''">
				AND STAFF_SCORE=#{visitScore.staffScore}
            </if>
            <if test="visitScore.totalScore!= null and visitScore.totalScore!= ''">
				AND TOTAL_SCORE=#{visitScore.totalScore}
            </if>
            <if test="visitScore.remark!= null and visitScore.remark!= ''">
				AND REMARK=#{visitScore.remark}
            </if>
            <if test="visitScore.createBy!= null and visitScore.createBy!= ''">
				AND CREATE_BY=#{visitScore.createBy}
            </if>
            <if test="visitScore.createTime!= null and visitScore.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{visitScore.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScore.updateBy!= null and visitScore.updateBy!= ''">
				AND UPDATE_BY=#{visitScore.updateBy}
            </if>
            <if test="visitScore.updateTime!= null and visitScore.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{visitScore.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScore.version!= null and visitScore.version!= ''">
				AND VERSION=#{visitScore.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'visitId'">
				VISIT_ID ${value}
            </when>
        	<when test="key == 'shopScore'">
				SHOP_SCORE ${value}
            </when>
        	<when test="key == 'saleScore'">
				SALE_SCORE ${value}
            </when>
        	<when test="key == 'staffScore'">
				STAFF_SCORE ${value}
            </when>
        	<when test="key == 'totalScore'">
				TOTAL_SCORE ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="VisitScore">
		<![CDATA[ INSERT INTO T_VISIT_SCORE ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{visitId}
			, #{shopId}
			, #{shopScore}
			, #{saleScore}
			, #{staffScore}
			, #{totalScore}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="VisitScore">
		<![CDATA[ UPDATE T_VISIT_SCORE SET 
			 ID=#{id} 
			, VISIT_ID=#{visitId}
			, SHOP_ID=#{shopId}  
			, SHOP_SCORE=#{shopScore} 
			, SALE_SCORE=#{saleScore} 
			, STAFF_SCORE=#{staffScore} 
			, TOTAL_SCORE=#{totalScore} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_VISIT_SCORE WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_VISIT_SCORE WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="VisitScore">
		DELETE FROM T_VISIT_SCORE WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_VISIT_SCORE
	</select>
	<select id="listByProperty" parameterType="VisitScore" resultMap="VisitScoreResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="VisitScore" resultMap="VisitScoreResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="VisitScore" resultType="int">
		SELECT count(1) AS totalcount FROM T_VISIT_SCORE WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	
	<select id="queryVisitScoreByVisitId" parameterType="java.lang.Long" resultMap="VisitScoreDtoResultMapper">
		SELECT * FROM T_VISIT_SCORE WHERE VISIT_ID = #{visitId}
	</select>
</mapper>