<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.transsion.store.mapper.VisitScoreSettingMapper">
	<resultMap id="VisitScoreSettingResultMapper" type="VisitScoreSetting">
		<result column="ID" property="id"/>
		<result column="COMPANY_ID" property="companyId"/>
		<result column="TYPE" property="type"/>
		<result column="DESC_EN" property="descEn"/>
		<result column="DESC_CN" property="descCn"/>
		<result column="REMARK" property="remark"/>
		<result column="CREATE_BY" property="createBy"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="UPDATE_BY" property="updateBy"/>
		<result column="UPDATE_TIME" property="updateTime"/>
		<result column="VERSION" property="version"/>
	</resultMap>
	
	<sql id="commonColumns">
		 ID
		, COMPANY_ID
		, TYPE
		, DESC_EN
		, DESC_CN
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>

	<sql id="commonColumnsNotPK">
		 COMPANY_ID
		, TYPE
		, DESC_EN
		, DESC_CN
		, REMARK
		, CREATE_BY
		, CREATE_TIME
		, UPDATE_BY
		, UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonProcessDateColumns">
		 ID
		, COMPANY_ID
		, TYPE
		, DESC_EN
		, DESC_CN
		, REMARK
		, CREATE_BY
		, date_format(CREATE_TIME, '%Y-%m-%d %H:%i:%s') CREATE_TIME
		, UPDATE_BY
		, date_format(UPDATE_TIME, '%Y-%m-%d %H:%i:%s') UPDATE_TIME
		, VERSION
	</sql>
	
	<sql id="commonCondition">
		<if test="id!= null and id!= ''"> 
			AND ID=#{id}
		</if>
		<if test="companyId!= null and companyId!= ''"> 
			AND COMPANY_ID=#{companyId}
		</if>
		<if test="type!= null and type!= ''"> 
			AND TYPE=#{type}
		</if>
		<if test="descEn!= null and descEn!= ''"> 
			AND DESC_EN=#{descEn}
		</if>
		<if test="descCn!= null and descCn!= ''"> 
			AND DESC_CN=#{descCn}
		</if>
		<if test="remark!= null and remark!= ''"> 
			AND REMARK=#{remark}
		</if>
		<if test="createBy!= null and createBy!= ''"> 
			AND CREATE_BY=#{createBy}
		</if>
		<if test="createTime!= null and createTime!= ''"> 
			AND CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="updateBy!= null and updateBy!= ''"> 
			AND UPDATE_BY=#{updateBy}
		</if>
		<if test="updateTime!= null and updateTime!= ''"> 
			AND UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
		</if>
		<if test="version!= null and version!= ''"> 
			AND VERSION=#{version}
		</if>
	</sql>

	<sql id="commonConditionByPrefix">
		<if test="visitScoreSetting != null">
            <if test="visitScoreSetting.id!= null and visitScoreSetting.id!= ''">
				AND ID=#{visitScoreSetting.id}
            </if>
            <if test="visitScoreSetting.companyId!= null and visitScoreSetting.companyId!= ''">
				AND COMPANY_ID=#{visitScoreSetting.companyId}
            </if>
            <if test="visitScoreSetting.type!= null and visitScoreSetting.type!= ''">
				AND TYPE=#{visitScoreSetting.type}
            </if>
            <if test="visitScoreSetting.descEn!= null and visitScoreSetting.descEn!= ''">
				AND DESC_EN=#{visitScoreSetting.descEn}
            </if>
            <if test="visitScoreSetting.descCn!= null and visitScoreSetting.descCn!= ''">
				AND DESC_CN=#{visitScoreSetting.descCn}
            </if>
            <if test="visitScoreSetting.remark!= null and visitScoreSetting.remark!= ''">
				AND REMARK=#{visitScoreSetting.remark}
            </if>
            <if test="visitScoreSetting.createBy!= null and visitScoreSetting.createBy!= ''">
				AND CREATE_BY=#{visitScoreSetting.createBy}
            </if>
            <if test="visitScoreSetting.createTime!= null and visitScoreSetting.createTime!= ''">
				AND CREATE_TIME=str_to_date(#{visitScoreSetting.createTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreSetting.updateBy!= null and visitScoreSetting.updateBy!= ''">
				AND UPDATE_BY=#{visitScoreSetting.updateBy}
            </if>
            <if test="visitScoreSetting.updateTime!= null and visitScoreSetting.updateTime!= ''">
				AND UPDATE_TIME=str_to_date(#{visitScoreSetting.updateTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="visitScoreSetting.version!= null and visitScoreSetting.version!= ''">
				AND VERSION=#{visitScoreSetting.version}
            </if>
        </if>
    </sql>

	<sql id="orderByClause">
		<if test="orderBy != null">
			<foreach collection="orderBy" index="key" item="value"
					 open=" order by " separator="," close=" ">
				<include refid="orderBy" />
			</foreach>
		</if>
	</sql>

	<sql id="orderBy">
		<choose>
        	<when test="key == 'id'">
				ID ${value}
            </when>
        	<when test="key == 'companyId'">
				COMPANY_ID ${value}
            </when>
        	<when test="key == 'type'">
				TYPE ${value}
            </when>
        	<when test="key == 'descEn'">
				DESC_EN ${value}
            </when>
        	<when test="key == 'descCn'">
				DESC_CN ${value}
            </when>
        	<when test="key == 'remark'">
				REMARK ${value}
            </when>
        	<when test="key == 'createBy'">
				CREATE_BY ${value}
            </when>
        	<when test="key == 'createTime'">
				CREATE_TIME ${value}
            </when>
        	<when test="key == 'updateBy'">
				UPDATE_BY ${value}
            </when>
        	<when test="key == 'updateTime'">
				UPDATE_TIME ${value}
            </when>
        	<when test="key == 'version'">
				VERSION ${value}
            </when>
		</choose>
	</sql>

	<insert id="save" useGeneratedKeys="true" keyProperty="id" parameterType="VisitScoreSetting">
		<![CDATA[ INSERT INTO T_VISIT_SCORE_SETTING ( ]]>
		<include refid="commonColumnsNotPK"/>
		<![CDATA[
			) VALUES ( 
			 #{companyId}
			, #{type}
			, #{descEn}
			, #{descCn}
			, #{remark}
			, #{createBy}
			, str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, #{updateBy}
			, str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, #{version}
  ) ]]>
	</insert>
	<update id="update" parameterType="VisitScoreSetting">
		<![CDATA[ UPDATE T_VISIT_SCORE_SETTING SET 
			 ID=#{id} 
			, COMPANY_ID=#{companyId} 
			, TYPE=#{type} 
			, DESC_EN=#{descEn} 
			, DESC_CN=#{descCn} 
			, REMARK=#{remark} 
			, CREATE_BY=#{createBy} 
			, CREATE_TIME=str_to_date(#{createTime},'%Y-%m-%d %H:%i:%s')
			, UPDATE_BY=#{updateBy} 
			, UPDATE_TIME=str_to_date(#{updateTime},'%Y-%m-%d %H:%i:%s')
			, VERSION=#{version} 
		WHERE ID = #{id}  ]]>
	</update>
	<delete id="deleteByPK" parameterType="java.lang.Long">
		<![CDATA[ DELETE FROM T_VISIT_SCORE_SETTING WHERE ID = #{id}  ]]>
	</delete>
	<delete id="deleteByPKeys" parameterType="map">
		DELETE FROM T_VISIT_SCORE_SETTING WHERE
		<foreach collection="primaryKeys" index="index" item="id"
			open=" ID IN(" separator="," close=") ">
			#{id}
		</foreach>
	</delete>
	<delete id="deleteByProperty" parameterType="VisitScoreSetting">
		DELETE FROM T_VISIT_SCORE_SETTING WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>
	<select id="getByPK" parameterType="java.lang.Long" resultMap="VisitScoreSettingResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE_SETTING WHERE ID = #{id}  
	</select>
	<select id="list" resultMap="VisitScoreSettingResultMapper">
		<![CDATA[ SELECT ]]>
			 <include refid="commonProcessDateColumns"/>
 		FROM T_VISIT_SCORE_SETTING
	</select>
	<select id="listByProperty" parameterType="VisitScoreSetting" resultMap="VisitScoreSettingResultMapper">
		<![CDATA[ SELECT ]]>
			<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE_SETTING WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
	<select id="listPaginationByProperty" parameterType="VisitScoreSetting" resultMap="VisitScoreSettingResultMapper">
		SELECT 
		<include refid="commonProcessDateColumns"/>
		FROM T_VISIT_SCORE_SETTING WHERE 1=1
    	<include refid="commonConditionByPrefix"/>
    	<include refid="orderByClause" />
	</select>
	<select id="findByCount" parameterType="VisitScoreSetting" resultType="int">
		SELECT count(1) AS totalcount FROM T_VISIT_SCORE_SETTING WHERE 1=1 
		<include refid="commonCondition"/>
	</select>
</mapper>